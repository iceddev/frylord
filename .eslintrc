{
  "rules": {
    "indent": [2, 2, { "SwitchCase": 1 }],
    "quotes": [2, "single"],
    "strict": [2, "global"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "arrow-parens": [2],
    "arrow-spacing": [2],
    "no-var": [2],
    "prefer-const": [2],
    "constructor-super": [2],
    "no-this-before-super": [2],
    "object-shorthand": [2, "always"],
    "space-in-parens": [2, "never"],
    "no-console": [1]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  }
}
